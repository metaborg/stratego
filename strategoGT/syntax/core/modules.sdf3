module core/modules

imports
  core/identifiers
  core/layout
  core/strategies
  core/signatures

sorts Module
context-free syntax

  Module.Module =
<module <ModName>

<{AnnoDecl "\n\n"}*>>

  Module.Specification =
<specification

<{AnnoDecl "\n\n"}*>>

sorts Decl AnnoDecl
context-free syntax
  Decl.Imports =
<imports

<{ImportModName "\n"}*>>

  Decl.Strategies = 
<strategies

<{Def "\n\n"}*>>

  Decl.Signature =
<signature
<{Sdecl "\n\n"}*>>

  AnnoDecl = Decl

sorts ImportModName
context-free syntax
  ImportModName.Import = ModName

syntax
  ImportModName-CF.ImportWildcard = ModName-CF "/" "-"


module Analysis

language

  observer : editor-analyze
  context  : legacy
  
references

  reference _ : editor-resolve
  hover _     : editor-hover

menus
  
  menu: "Analysis" (openeditor)

    action: "Show analyzed AST" = debug-show-analyzed
    action: "Show program with explicit casts" = debug-show-analyzed-concrete
    action: "Test cast correct" = test-cast-correct
    action: "Test cast incorrect" = test-cast-incorrect

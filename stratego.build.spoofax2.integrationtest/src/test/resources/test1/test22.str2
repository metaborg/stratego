module test22
imports libstratego-lib
signature
  constructors
    Foo: String -> Fnord
    Bar: String * String -> Fnord

strategies

  main = 
    !Foo("uck")
    ; check(test1)
    ; check(test3)

  check(s) = 
  where( debug(!"doing test on: ")
  ; s
  ; debug (!"succeeded with: ")
  <+ say (!"failed")
  )

  test1 :
    Foo(a) -> Bar(a,"B")

  test3 :
    Foo(a) -> Foo(a)
    where !"bla"
	; debug(!"tekst: ")


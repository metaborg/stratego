module test89
imports libstratego-lib
strategies

  main =
    <all-G-by-H> H(G(G(H(G(B(), I(1)), A()), B()), I(0)), A())
	      => H(G(G(H(H(B(), I(1)), A()), B()), I(0)), A())

  all-G-by-H = 
    bottomup-term(try(?G(x, y); !H(x, y)))

strategies

  all-term(s) = (A() + B() + F(s, s) + G(s, s) + H(s, s) + I(id))
    bottomup-term(s) = all-term(bottomup-term(s)) ; s

signature
   sorts Term
   constructors
     A : Term
     B : Term
     F : Term * Term -> Term
     G : Term * Term -> Term
     H : Term * Term -> Term
     I : Int -> Term

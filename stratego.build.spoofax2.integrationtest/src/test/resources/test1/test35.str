strategies

  Hd     : [x | _] -> x
  Tl     : [_ | l] -> l

  member = 
    (?x, fetch(?x))

  main =
    <not(member)> ("a", ["d", "b", "c"])

  fetch(s) =
    rec x([s | id] <+ [id | x])

  main' =
    <different> ["F","G","X","e_0","f_0","d_0"]

  different =
    ([] + where(!(<Hd>, <Tl>); not(member))
         ; where(Tl; different))

    

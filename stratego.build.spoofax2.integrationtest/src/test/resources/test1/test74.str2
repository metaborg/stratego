module test74
imports libstratego-lib
strategies

  main = 
    test-suite(!"test74",
      test1
    )

  test1 = 
    apply-test(!"test1"
	, define-rule
          ; transform
          ; test-rule
	, !"a"
	, !("a", "a", "b")
    )


  define-rule =
    ?x
    ; rules( A : (x, _, _) )

  transform = 
    !(<id>, <id>, "b")

  test-rule = 
    A

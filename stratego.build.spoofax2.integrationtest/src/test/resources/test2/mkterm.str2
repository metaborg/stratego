module mkterm
imports libstratego-lib
signature
  constructors
    F : Term * Term * Term -> Term
    X : Term
    Y : Term
    Z : Term

rules

  A : f#( xs ) -> f#(xs)

  B : f#(xs) -> "b"#(xs)

  C : "b"#(xs) -> "c"#(xs)

  D : f#(xs) -> [f | xs]

strategies

  main = 
    <A; B; C; id#([id|id]); D> F(X(),Y(),Z()) => ["c",X(),Y(),Z()] 

rules // Debugging

  debug-show-aterm :: ? * ? * ? * string * string -> string * ?
  debug-show-aterm: (selected, _, _, path, project-path) -> (filename, result)
    with filename := <guarantee-extension(|"aterm")> path
       ; result   := selected

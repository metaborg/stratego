module multimatch2matrix

imports
  strj/multimatch/matrix

  signatures/core/strategies-sig

strategies
  /*======== Construct clause and action matrices ========*/
  multimatch2matrix: MultiMatch(Linear(), cases) -> Matrix(rows)
  where
    <cases2rows> cases => rows

  cases2rows = map(case2row)

  case2row: MatchCase(t, s) -> Row([t], s)

  case2row = debug(!"case2row fail on: "); fail

module stratego/core/strategies

imports stratego/core/terms

namespaces Strategy StrategyVariable MatchContext BuildContext

binding rules

  SDefNoArgs(name, _) : 
  	defines Strategy name 
  	scopes Variable
  	
  SVar(name) :
  	refers to Strategy name
  	
  DefaultVarDec(name) :
  	defines Strategy name
  	
  Match(_) :
  	implicitly defines MatchContext ""
  
  Build(_) :
  	implicitly defines BuildContext ""
  	
  Scope(_, _) :
  	scopes Variable
  	
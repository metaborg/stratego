module test36
imports collection/list/cons collection/tuple/cons
signature
  constructors
    Stream : a -> b
    stdout : FILE

strategies

  main = 
    ?foo
    ; <fclose> Stream(0)
    ; gclose(|1)
    ; !foo

  fclose =
    try(?Stream(o))

  gclose( | foo) =
    rec x(
    { bar : 
      !foo
      ; ?bar
    }
    )

  try(s) = s <+ id

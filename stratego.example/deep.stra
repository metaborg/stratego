module deep

signature
  sorts List
  constructors
    Nil   :                List(a)
    Cons  : a * List(a) -> List(a)

strategies
  length = match sequential
             case | Nil() where id: !0
             case | Nil() where fail: !0
             case | Cons(_, Nil()): !1
             case | Cons(_, Cons(_, Nil())): !2
             case | Cons(_, Cons(_, Cons(_, Nil()))): !3
             case | Cons(_, _): !4
           end

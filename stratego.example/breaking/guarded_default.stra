module guarded_default

signature
  sorts List
  constructors
    Nil   :                List(a)
    Cons  : a * List(a) -> List(a)

strategies
  length(s) = match sequential
               case | Nil(): !0
               case | Cons(_, Nil()): !1
               case | _ where all(s): !2
               case | _ where id: !3
               case | _ where fail: !3
               case | _ where some(s): !3
             end

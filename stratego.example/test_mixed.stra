module test_mixed

signature
  constructors
    Nil 	: 				         List(a)
    Cons 	: a * List(a) 	-> List(a)

strategies
  uncar2 = match sequential
                 case   | Cons(_, _): !1 // equality check
                 case y | Cons(y, _): !y // matches all Cons(_, _)
                 case z | Cons(Nil(), z): !z // dead
              end 


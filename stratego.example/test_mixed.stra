module test_mixed

signature
  constructors
    Nil 	: 				         List(a)
    Cons 	: a * List(a) 	-> List(a)
    Car1  : a             -> List(a)
    Car2  : a             -> List(a)

strategies
  uncar2 = match sequential
             case | Car1(a): Cons(!1, Nil())
             case | Car2(b): Cons(!2, Nil())
             case | "abc": !"cba"
             case | 8: !54
             case | 5.4: !8
           end


module test_mixed

signature
  constructors
    Nil 	: 				         List(a)
    Cons 	: a * List(a) 	-> List(a)

    Car1 	: a 			      -> List(a)
    Car2  : a             -> List(a)

strategies
  uncar2(|x) = match sequential
                 case Cons(x, _): !x // equality check
                 case Cons(y, _): !y // matches all Cons(_, _)
                 case Cons(Nil(), z): !z // dead
              end


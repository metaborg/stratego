module exploded
imports libstratego-lib

strategies
  s( | ) =
    match sequential
      case | "Foo"#([]): !1 // Foo
      case | "Bar"#([1{"anno"}, 2]): !2
      case | "\"Foo\\_\""#([]): !3 // "Foo"
      case | 1#([]): !4
      case | 5.4#([]): !5
      case foo | foo#([1, "x"]): !6
      case | []#([1, 2, 3]): !7
      case | "Nil"#([]): !8
      case | "Cons"#([1, 2, 3]): !9
      case | "Nil"#(["not", "a", "list"]): !10
    end
